<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.wahyou.simpanq">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="id.wahyou.simpanq.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimpanQ">
        <activity android:name="id.wahyou.simpanq.ui.plan.edit.EditPlanActivity" android:screenOrientation="portrait"/>
        <activity android:name="id.wahyou.simpanq.ui.plan.add.AddPlanActivity" android:screenOrientation="portrait"/>
        <activity android:name="id.wahyou.simpanq.ui.MainActivity" android:screenOrientation="portrait"/>
        <activity android:name="id.wahyou.simpanq.ui.link.edit.EditLinkActivity" android:screenOrientation="portrait"/>
        <activity android:name="id.wahyou.simpanq.ui.link.add.AddLinkActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="id.wahyou.simpanq.ui.splash.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>